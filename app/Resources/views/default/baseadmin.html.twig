<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %} {% endblock %}</title>
    <link rel="stylesheet" href="{{ asset('bundles/perfectcity/bootstrap/css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css') }}">
    <link rel="stylesheet" href="{{ asset('https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/perfectcity/plugins/jvectormap/jquery-jvectormap-1.2.2.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/perfectcity/dist/css/AdminLTE.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/perfectcity/dist/css/skins/_all-skins.min.css') }}">
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/perfectcity/Image/icon.png') }}" />

    {% block stylesheets %}{% endblock %}

</head>
<body class="hold-transition skin-blue layout-top-nav" >
<div class="wrapper">
    <header class="main-header">
        <nav class="navbar navbar-static-top">
            <div class="container">
                <div class="navbar-header">
                    <img src="{{ asset('bundles/perfectcity/dist/img/logo.png')}}" alt="smoothy html5 template" width="150px" height="50px">              <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                        <i class="fa fa-bars"></i>
                    </button>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
                    <ul class="nav navbar-nav">

                    </ul>
                    <form class="navbar-form navbar-left" role="search">
                        <div class="form-group">
                            <input type="text" class="form-control" id="navbar-search-input" placeholder="Recherche">
                        </div>
                    </form>
                </div><!-- /.navbar-collapse -->
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">

                        <!-- User Account Menu -->
                        <li class="dropdown user user-menu">
                            <!-- Menu Toggle Button -->
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="hidden-xs"> <i class="fa fa-user"></i>{{ app.session.get('username').username }}</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- The user image in the menu -->
                                <li class="user-header">
                                    <p>
                                        <small>Bienvenue</small>
                                    </p>
                                </li>
                                <!-- Menu Body -->

                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="{{ path('profiladmin') }}" class="btn btn-default btn-flat">Profil</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="{{ path('logout') }}" class="btn btn-default btn-flat">Deconnexion</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div><!-- /.navbar-custom-menu -->
            </div><!-- /.container-fluid -->
        </nav>
    </header>
    <!-- Full Width Column -->
    <div class="content-wrapper">
        <div class="container">




            {% block body %}{% endblock %}



        </div><!-- /.container -->
    </div><!-- /.content-wrapper -->





    <footer class="main-footer">
        <div class="pull-right hidden-xs">
            <b>Tunisie</b>
        </div>
        <strong>droits d'auteur 2018 <a href="#"></a>.</strong>Tous les droits réservés
    </footer>
    <!-- Add the sidebar's background. This div must be placed
         immediately after the control sidebar -->
    <div class="control-sidebar-bg"></div>
</div><!-- ./wrapper -->



<script src="{{ asset('bundles/perfectcity/plugins/jQuery/jQuery-2.1.4.min.js') }}"></script>
{% block javascripts %}{% endblock %}
{% block piquer %}{% endblock %}
<script src="{{ asset('bundles/perfectcity/bootstrap/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/perfectcity/plugins/fastclick/fastclick.min.js') }}"></script>
<script src="{{ asset('bundles/perfectcity/dist/js/app.min.js') }}"></script>
<script src="{{ asset('bundles/perfectcity/plugins/sparkline/jquery.sparkline.min.js') }}"></script>
<script src="{{ asset('bundles/perfectcity/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
<script src="{{ asset('bundles/perfectcity/plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
<script src="{{ asset('bundles/perfectcity/plugins/slimScroll/jquery.slimscroll.min.js') }}"></script>
<script src="{{ asset('bundles/perfectcity/plugins/chartjs/Chart.min.js') }}"></script>
<script src="{{ asset('bundles/perfectcity/dist/js/pages/dashboard2.js') }}"></script>
<script src="{{ asset('bundles/perfectcity/dist/js/demo.js') }}"></script>

</body>
</html>
